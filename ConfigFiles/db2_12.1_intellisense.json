      "TABCONST", "REFERENCES", "CHECKS",
      "SEQUENCES", "DATATYPES",
      "SCHEMATA", "TABLESPACES",
      "DBAUTH", "TABAUTH", "COLAUTH", "ROUTINEAUTH", "SCHEMAAUTH", "PACKAGEAUTH",
      "ROLES", "ROLEAUTH",
      "BUFFERPOOLS", "PARTITIONMAPS",
      "TABDEP", "COLDIST", "COLGROUPS"
    ],
    "SYSIBM": [
      "SYSDUMMY1", "DUAL",
      "SYSTABLES", "SYSCOLUMNS", "SYSINDEXES", "SYSVIEWS",
      "SYSROUTINES", "SYSTRIGGERS", "SYSSEQUENCES"
    ],
    "SYSSTAT": [
      "TABLES", "COLUMNS", "INDEXES", "COLDIST", "COLGROUPS"
    ],
    "SYSIBMADM": [
      "APPLICATIONS", "SNAPLOCK", "SNAPLOCKWAIT",
      "DBCFG", "DBMCFG", "REG_VARIABLES",
      "TBSP_UTILIZATION", "CONTAINER_UTILIZATION",
      "TOP_DYNAMIC_SQL", "QUERY_PREP_COST"
    ]
  },
  
  "snippets": {
    "selectAll": {
      "trigger": "sel*",
      "template": "SELECT * FROM ${1:table_name} WHERE ${2:condition}",
      "description": "Select all columns from table"
    },
    "selectColumns": {
      "trigger": "selc",
      "template": "SELECT ${1:column1}, ${2:column2}\nFROM ${3:table_name}\nWHERE ${4:condition}",
      "description": "Select specific columns"
    },
    "selectJoin": {
      "trigger": "selj",
      "template": "SELECT ${1:columns}\nFROM ${2:table1} t1\nINNER JOIN ${3:table2} t2 ON t1.${4:key} = t2.${5:key}\nWHERE ${6:condition}",
      "description": "Select with join"
    },
    "selectCount": {
      "trigger": "selcnt",
      "template": "SELECT COUNT(*) AS cnt FROM ${1:table_name} WHERE ${2:condition}",
      "description": "Count rows"
    },
    "selectGroupBy": {
      "trigger": "selg",
      "template": "SELECT ${1:column}, COUNT(*) AS cnt\nFROM ${2:table_name}\nGROUP BY ${1:column}\nORDER BY cnt DESC",
      "description": "Group by with count"
    },
    "insertInto": {
      "trigger": "ins",
      "template": "INSERT INTO ${1:table_name} (${2:columns})\nVALUES (${3:values})",
      "description": "Insert single row"
    },
    "insertSelect": {
      "trigger": "inss",
      "template": "INSERT INTO ${1:target_table} (${2:columns})\nSELECT ${3:columns}\nFROM ${4:source_table}\nWHERE ${5:condition}",
      "description": "Insert from select"
    },
    "updateSet": {
      "trigger": "upd",
      "template": "UPDATE ${1:table_name}\nSET ${2:column} = ${3:value}\nWHERE ${4:condition}",
      "description": "Update rows"
    },
    "deleteFrom": {
      "trigger": "del",
      "template": "DELETE FROM ${1:table_name}\nWHERE ${2:condition}",
      "description": "Delete rows"
    },
    "merge": {
      "trigger": "merge",
      "template": "MERGE INTO ${1:target_table} t\nUSING ${2:source_table} s\nON t.${3:key} = s.${4:key}\nWHEN MATCHED THEN UPDATE SET ${5:column} = s.${6:column}\nWHEN NOT MATCHED THEN INSERT (${7:columns}) VALUES (${8:values})",
      "description": "Merge (upsert)"
    },
    "createTable": {
      "trigger": "crtab",
      "template": "CREATE TABLE ${1:schema}.${2:table_name} (\n    ${3:id} INTEGER NOT NULL GENERATED ALWAYS AS IDENTITY,\n    ${4:column} VARCHAR(100),\n    PRIMARY KEY (${3:id})\n)",
      "description": "Create table"
    },
    "createIndex": {
      "trigger": "cridx",
      "template": "CREATE INDEX ${1:index_name}\nON ${2:table_name} (${3:columns})",
      "description": "Create index"
    },
    "createView": {
      "trigger": "crvw",
      "template": "CREATE OR REPLACE VIEW ${1:schema}.${2:view_name} AS\nSELECT ${3:columns}\nFROM ${4:table_name}\nWHERE ${5:condition}",
      "description": "Create view"
    },
    "cte": {
      "trigger": "with",
      "template": "WITH ${1:cte_name} AS (\n    SELECT ${2:columns}\n    FROM ${3:table_name}\n    WHERE ${4:condition}\n)\nSELECT * FROM ${1:cte_name}",
      "description": "Common Table Expression"
    },
    "pagination": {
      "trigger": "page",
      "template": "SELECT ${1:columns}\nFROM ${2:table_name}\nORDER BY ${3:column}\nOFFSET ${4:0} ROWS FETCH FIRST ${5:10} ROWS ONLY",
      "description": "Pagination query"
    },
    "rowNumber": {
      "trigger": "rownum",
      "template": "SELECT ROW_NUMBER() OVER (ORDER BY ${1:column}) AS rn, ${2:columns}\nFROM ${3:table_name}",
      "description": "Row number window function"
    }
  },
  
  "systemSchemas": [
    "SYSCAT", "SYSIBM", "SYSIBMADM", "SYSSTAT", "SYSPROC", "SYSFUN", "SYSTOOLS",
    "NULLID", "SQLJ", "DB2INST1"
  ],
  
  "specialValues": [
    "NULL", "TRUE", "FALSE", "UNKNOWN",
    "CURRENT DATE", "CURRENT TIME", "CURRENT TIMESTAMP",
    "CURRENT USER", "SESSION_USER", "SYSTEM_USER",
    "CURRENT SCHEMA", "CURRENT PATH"
  ]
}
