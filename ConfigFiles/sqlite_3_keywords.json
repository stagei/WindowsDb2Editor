{
  "provider": "SQLITE",
  "version": "3",
  "description": "SQLite SQL keywords and reserved words for intellisense and syntax highlighting",
  "keywords": {
    "statements": [
      "SELECT", "INSERT", "UPDATE", "DELETE",
      "CREATE", "ALTER", "DROP",
      "BEGIN", "END", "COMMIT", "ROLLBACK", "SAVEPOINT", "RELEASE",
      "EXPLAIN", "ANALYZE", "REINDEX", "VACUUM",
      "WITH", "RECURSIVE"
    ],
    "clauses": [
      "FROM", "WHERE", "GROUP BY", "HAVING", "ORDER BY",
      "JOIN", "INNER JOIN", "LEFT JOIN", "LEFT OUTER JOIN", "CROSS JOIN",
      "ON", "USING", "NATURAL",
      "UNION", "UNION ALL", "INTERSECT", "EXCEPT",
      "WITH", "AS",
      "INTO", "VALUES",
      "LIMIT", "OFFSET"
    ],
    "datatypes": [
      "INTEGER", "INT", "TINYINT", "SMALLINT", "MEDIUMINT", "BIGINT",
      "REAL", "DOUBLE", "FLOAT",
      "TEXT", "CHAR", "VARCHAR", "CLOB",
      "BLOB",
      "NUMERIC", "DECIMAL",
      "DATE", "DATETIME",
      "BOOLEAN", "BOOLEAN"
    ],
    "functions": [
      "COUNT", "SUM", "AVG", "MIN", "MAX", "TOTAL", "GROUP_CONCAT",
      "SUBSTR", "SUBSTRING", "TRIM", "LTRIM", "RTRIM",
      "UPPER", "LOWER", "LENGTH", "CHAR_LENGTH",
      "COALESCE", "NULLIF", "IFNULL",
      "CASE", "WHEN", "THEN", "ELSE",
      "CAST", "DATE", "DATETIME", "TIME", "STRFTIME", "JULIANDAY",
      "ROW_NUMBER", "RANK", "DENSE_RANK",
      "ABS", "ROUND", "RANDOM", "TYPEOF", "INSTR", "REPLACE", "HEX", "ZEROBLOB"
    ],
    "operators": [
      "AND", "OR", "NOT",
      "IN", "NOT IN", "EXISTS", "NOT EXISTS",
      "BETWEEN", "LIKE", "GLOB", "REGEXP", "MATCH", "IS NULL", "IS NOT NULL",
      "DISTINCT", "ALL", "ANY", "SOME"
    ],
    "system_tables": [
      "sqlite_master", "sqlite_schema",
      "pragma_table_info", "pragma_table_list", "pragma_foreign_key_list",
      "pragma_index_list", "pragma_index_info"
    ],
    "constraints": [
      "PRIMARY KEY", "FOREIGN KEY", "UNIQUE", "CHECK",
      "NOT NULL", "DEFAULT",
      "REFERENCES", "CONSTRAINT",
      "ON CONFLICT", "AUTOINCREMENT", "COLLATE"
    ],
    "modifiers": [
      "ASC", "DESC",
      "FOR UPDATE", "FOR SHARE",
      "TEMP", "TEMPORARY", "IF NOT EXISTS", "IF EXISTS"
    ]
  }
}
