    "GetTableDependencies": {
      "sql": "SELECT BSCHEMA, BNAME, BTYPE, DTYPE FROM SYSCAT.TABDEP WHERE TABSCHEMA = ? AND TABNAME = ? ORDER BY BSCHEMA, BNAME",
      "description": "Get object dependencies (objects that this table/view depends on). Returns target schema, object name, type, and dependency type from SYSCAT.TABDEP. Used by CLI command 'dependencies'.",
      "parameters": ["TABSCHEMA", "TABNAME"],
      "returns": "List of dependencies with target objects",
      "source": "CliCommandHandlerService:GetDependenciesAsync"
    },
    "GetCdcInfo": {
      "sql": "SELECT TABSCHEMA, TABNAME, PROPERTY FROM SYSCAT.TABLES WHERE TABSCHEMA LIKE ? AND TYPE IN ('T', 'U') AND PROPERTY IS NOT NULL ORDER BY TABSCHEMA, TABNAME",
