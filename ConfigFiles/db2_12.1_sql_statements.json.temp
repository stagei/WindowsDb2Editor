      "returns": "List of views with metadata",
      "source": "CliCommandHandlerService:ListViewsAsync"
    },
    "ListProcedures": {
      "sql": "SELECT PROCSCHEMA, PROCNAME, LANGUAGE, DETERMINISTIC FROM SYSCAT.PROCEDURES WHERE PROCSCHEMA LIKE ? ORDER BY PROCSCHEMA, PROCNAME",
      "description": "List all stored procedures in schema(s). Returns procedure schema, name, language, and deterministic flag. Schema filter supports wildcards. Used by CLI command 'list-procedures'.",
      "parameters": ["PROCSCHEMA_PATTERN"],
      "returns": "List of procedures with metadata",
      "source": "CliCommandHandlerService:ListProceduresAsync"
