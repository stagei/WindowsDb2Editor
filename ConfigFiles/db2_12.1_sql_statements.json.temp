    "ListTriggers": {
      "sql": "SELECT TRIGSCHEMA, TRIGNAME, TABSCHEMA, TABNAME, TRIGTIME, TRIGEVENT, ENABLED FROM SYSCAT.TRIGGERS WHERE TRIGSCHEMA LIKE ? ORDER BY TRIGSCHEMA, TRIGNAME",
      "description": "List all triggers in schema(s). Returns trigger schema, name, target table, timing (BEFORE/AFTER), event (INSERT/UPDATE/DELETE), and enabled flag. Schema filter supports wildcards. Used by CLI command 'list-triggers'.",
