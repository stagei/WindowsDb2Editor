      "returns": "List of packages with metadata",
      "source": "CliCommandHandlerService:ListPackagesAsync"
    },
    "GetObjectComment": {
      "sql": "SELECT TRIM(TABSCHEMA) AS Schema, TRIM(TABNAME) AS Name, COALESCE(TRIM(REMARKS), '') AS Comment FROM SYSCAT.TABLES WHERE TABSCHEMA = ? AND TABNAME = ?",
      "description": "Get comment/remarks for a specific table. Returns schema, name, and comment text. Used by CLI command 'object-comment'.",
      "parameters": ["TABSCHEMA", "TABNAME"],
