    "GetPackageStatements": {
      "sql": "SELECT CAST(X.STMTNO AS VARCHAR(10)) AS STMTNO, CAST(X.SECTNO AS VARCHAR(10)) AS SECTNO, CAST(X.SEQNO AS VARCHAR(10)) AS SEQNO, TRIM(X.TEXT) AS STATEMENT_TEXT FROM SYSCAT.STATEMENTS X JOIN SYSCAT.PACKAGES Z ON TRIM(X.PKGSCHEMA) = TRIM(Z.PKGSCHEMA) AND TRIM(X.PKGNAME) = TRIM(Z.PKGNAME) WHERE TRIM(X.PKGSCHEMA) = ? AND TRIM(X.PKGNAME) = ? ORDER BY X.STMTNO, X.SECTNO, X.SEQNO",
