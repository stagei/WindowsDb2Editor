      "parameters": ["TABSCHEMA", "TABNAME"],
      "returns": "List of foreign keys with full constraint details",
      "source": "TableDetailsDialog:LoadForeignKeysAsync"
    },
    "GetTableIndexes_Aggregated": {
      "sql": "SELECT TRIM(I.INDNAME) AS IndexName, CASE WHEN I.INDEXTYPE = 'CLUS' THEN 'Clustered' WHEN I.INDEXTYPE = 'REG' THEN 'Regular' WHEN I.INDEXTYPE = 'DIM' THEN 'Dimension' ELSE I.INDEXTYPE END AS IndexType, CASE WHEN I.UNIQUERULE = 'U' THEN 'Yes' WHEN I.UNIQUERULE = 'P' THEN 'Primary Key' ELSE 'No' END AS IsUnique, LISTAGG(TRIM(IC.COLNAME), ', ') WITHIN GROUP (ORDER BY IC.COLSEQ) AS Columns FROM SYSCAT.INDEXES I LEFT JOIN SYSCAT.INDEXCOLUSE IC ON I.INDSCHEMA = IC.INDSCHEMA AND I.INDNAME = IC.INDNAME WHERE TRIM(I.TABSCHEMA) = ? AND TRIM(I.TABNAME) = ? GROUP BY I.INDNAME, I.INDEXTYPE, I.UNIQUERULE ORDER BY I.INDNAME",
      "description": "Get all indexes for a table with aggregated column lists. Uses LISTAGG to concatenate column names. Returns index name, type (user-friendly labels), uniqueness status, and comma-separated columns. Used by TableDetailsDialog Tab 3 (Indexes).",
      "parameters": ["TABSCHEMA", "TABNAME"],
      "returns": "List of indexes with column lists",
      "source": "TableDetailsDialog:LoadIndexesAsync"
    },
