{
  "provider": "DB2",
  "version": "12.1",
  "description": "DB2 SQL keywords and reserved words for intellisense and syntax highlighting",
  "keywords": {
    "statements": [
      "SELECT", "INSERT", "UPDATE", "DELETE", "MERGE",
      "CREATE", "ALTER", "DROP", "TRUNCATE",
      "BEGIN", "END", "COMMIT", "ROLLBACK",
      "GRANT", "REVOKE",
      "CALL", "RETURN",
      "DECLARE", "SET",
      "FETCH", "OPEN", "CLOSE"
    ],
    "clauses": [
      "FROM", "WHERE", "GROUP BY", "HAVING", "ORDER BY",
      "JOIN", "INNER JOIN", "LEFT JOIN", "RIGHT JOIN", "FULL JOIN", "CROSS JOIN",
      "ON", "USING",
      "UNION", "UNION ALL", "INTERSECT", "EXCEPT",
      "WITH", "AS",
      "INTO", "VALUES",
      "LIMIT", "OFFSET", "FETCH FIRST", "ROWS ONLY"
    ],
    "datatypes": [
      "INTEGER", "INT", "SMALLINT", "BIGINT",
      "DECIMAL", "NUMERIC", "DEC",
      "REAL", "DOUBLE", "FLOAT",
      "CHAR", "CHARACTER", "VARCHAR",
      "CLOB", "BLOB", "DBCLOB",
      "DATE", "TIME", "TIMESTAMP",
      "XML",
      "BOOLEAN"
    ],
    "functions": [
      "COUNT", "SUM", "AVG", "MIN", "MAX",
      "SUBSTR", "SUBSTRING", "TRIM", "RTRIM", "LTRIM",
      "UPPER", "LOWER", "LENGTH",
      "COALESCE", "NULLIF", "CASE", "WHEN", "THEN", "ELSE",
      "CAST", "CONVERT",
      "CURRENT DATE", "CURRENT TIME", "CURRENT TIMESTAMP",
      "YEAR", "MONTH", "DAY", "HOUR", "MINUTE", "SECOND",
      "LISTAGG", "ROW_NUMBER", "RANK", "DENSE_RANK"
    ],
    "operators": [
      "AND", "OR", "NOT",
      "IN", "NOT IN", "EXISTS", "NOT EXISTS",
      "BETWEEN", "LIKE", "IS NULL", "IS NOT NULL",
      "DISTINCT", "ALL", "ANY", "SOME"
    ],
    "system_tables": [
      "SYSCAT.TABLES", "SYSCAT.COLUMNS", "SYSCAT.INDEXES",
      "SYSCAT.VIEWS", "SYSCAT.ROUTINES", "SYSCAT.TRIGGERS",
      "SYSCAT.SEQUENCES", "SYSCAT.PACKAGES", "SYSCAT.TABLESPACES",
      "SYSCAT.SCHEMATA", "SYSCAT.DBAUTH", "SYSCAT.TABAUTH",
      "SYSCAT.KEYCOLUSE", "SYSCAT.REFERENCES", "SYSCAT.TABCONST",
      "SYSIBM.SYSDUMMY1",
      "SYSIBMADM.SNAPLOCK", "SYSIBMADM.SNAPAPPL_INFO"
    ],
    "constraints": [
      "PRIMARY KEY", "FOREIGN KEY", "UNIQUE", "CHECK",
      "NOT NULL", "DEFAULT",
      "REFERENCES", "CONSTRAINT"
    ],
    "modifiers": [
      "ASC", "DESC",
      "NULLS FIRST", "NULLS LAST",
      "FOR READ ONLY", "FOR UPDATE",
      "WITH UR", "WITH CS", "WITH RS", "WITH RR"
    ]
  }
}

